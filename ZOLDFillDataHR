  <!-- Javascript untuk add row -->
  <script>
    document.addEventListener('DOMContentLoaded', function () {
      // Add event listener to the add row button
      document.getElementById('addRowBtn').addEventListener('click', function () {
        addRow();
      });

      // Add event listener to the delete row button
      document.getElementById('deleteRow').addEventListener('click', function () {
        var lastRow = document.querySelector('#dataTable tbody tr:last-child');
        removeRow(lastRow);
      });
    });

    // Function to add a new row
    function addRow() {
      var tableBody = document.querySelector('#dataTable tbody');
      var newRow = tableBody.insertRow();
      newRow.innerHTML = `
              <td class="min-width">
                  <input type="text" name="name[]" class="form-control" required>
              </td>
              <td class="min-width">
                  <input type="email" name="division[]" class="form-control" required>
              </td>
              <td class="min-width">
                  <input type="tel" name="position[]" class="form-control" required>
              </td>
              <td class="min-width">
                  <input type="text" name="promoted[]" class="form-control" required>
              </td>
              <td class="min-width">
                  <input type="text" name="supervisor[]" class="form-control" required>
              </td>
              <td>
                <div class="action">
                    <button class="text-danger" onclick="removeRow(this)">
                        <i class="lni lni-trash-can"></i>
                    </button>
                </div>
            </td>`;
    }

    // Function to remove a row
    function removeRow(button) {
      var row = button.closest('tr');
      row.parentNode.removeChild(row);
    }
  </script>

   <!-- Javascript untuk hide and show -->
   <script>
    document.addEventListener("DOMContentLoaded", function () {
        const monthSelect = document.getElementById("monthSelect");
        const yearSelect = document.getElementById("yearSelect");
        const selectButton = document.getElementById("selectButton");
        const fileUploadColumn = document.getElementById("fileUploadColumn");
        const submitButton = document.getElementById("submitButton");

        let selectedMonth = "";
        let selectedYear = "";

        // Function to show/hide second column and store selected month/year
        function handleSelect() {
            selectedMonth = monthSelect.value;
            selectedYear = yearSelect.value;
            fileUploadColumn.style.display = "block";
            selectButton.disabled = true; // Disable select button after clicked
            if (selectedMonth && selectedYear) {
                submitButton.disabled = false;
            }
        }

        // Event listener for select button
        selectButton.addEventListener("click", handleSelect);


        if (selectedMonth && selectedYear) {
            monthSelect.value = selectedMonth;
            yearSelect.value = selectedYear;
            fileUploadColumn.style.display = "block";
            selectButton.disabled = true;
        }
    });
</script>


